{
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.3",
    "psr/container": "^2.0",
    "symfony/mailer": "^6.3",
    "doctrine/dbal": "^3.7",
    "doctrine/orm": "2.16.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Domain\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3"
  }
}
